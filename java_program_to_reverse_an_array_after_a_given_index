public class Main
{
	public static void main(String[] args) {
	    int[] arr={2,3,4,5,6,7};
	    System.out.println("enter the index after which you want to reverse the array");
	    for(int i=0;i<=key;i++){
	        System.out.println(arr[i]);
	        
	    }
	    for(int j=arr.length-1;j>key;j--){
	        System.out.println(arr[j]);
	    }
	
	}
